package fairytales.fairyDuckling{	import fairytales.Fairytale;	import fairytales.Platform;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;		public class FairyDuckling extends Fairytale	{				public function FairyDuckling(stage:Object)		{			gameStage = stage;				addEventListener(Event.ADDED_TO_STAGE, addedToStageHandler);			}				override public function buildObjects():void		{			addBackground(new DucklingBackground());			addPlatforms();			addHero(new Duckling());			addVillain(new DucklingVillain());			addPause();		}				override public function addPlatform():void		{			platform = new Rock();		}					override public function addGameOverScreen(value:Number):void		{			gameOverScreen = new DucklingGameOverScreen(value);		}		}}