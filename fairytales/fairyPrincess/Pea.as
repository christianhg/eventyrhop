package fairytales.fairyPrincess{	import fairytales.Villain; 		public class Pea extends Villain	{		//Embed MovieClip		[Embed(source="assets/pea.swf", symbol="Pea_mc")]		public static var PeaMC:Class;				public function Pea()		{		}				override public function drawVillain():void		{			view = new PeaMC();			view.x = -view.width * 0.5;			view.y = -view.height * 0.5;			view.gotoAndStop("front");			this.addChild(view);			height = view.height;		}		}}