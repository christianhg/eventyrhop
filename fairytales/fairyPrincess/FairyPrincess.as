package fairytales.fairyPrincess{	import fairytales.Fairytale;	import fairytales.Platform;		import flash.display.Sprite;	import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;		public class FairyPrincess extends Fairytale	{				public function FairyPrincess(stage:Object)		{			gameStage = stage;				addEventListener(Event.ADDED_TO_STAGE, addedToStageHandler);			}				override public function buildObjects():void		{			addBackground(new PrincessBackground());			addPlatforms();			addHero(new Princess());			addVillain(new Pea());			addPause();		}				override public function addPlatform():void		{			platform = new Pillow();		}					override public function addGameOverScreen(value:Number):void		{			gameOverScreen = new PrincessGameOverScreen(value);		}		}}