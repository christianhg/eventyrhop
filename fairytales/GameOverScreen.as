package fairytales{	import flash.display.DisplayObject;	import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.KeyboardEvent;	import flash.events.MouseEvent;	import flash.ui.Keyboard;	import utilities.EasyText;		public class GameOverScreen extends Sprite	{						[Embed(source="assets/images/gameOverBg.png")]		private var GameOverBg:Class;				[Embed(source="assets/images/gameOverHeader.png")]		private var GameOverHeaderImage:Class;				[Embed(source="assets/images/backToMenu.png")]		private var BackToMenuImage:Class;				[Embed(source="assets/images/playAgain.png")]		private var PlayAgainImage:Class;				[Embed(source="assets/images/addHighscore.png")]		private var AddHighscoreImage:Class;				private var _gameOverBg:DisplayObject;		private var _gameOver:Sprite = new Sprite();				private var _gameOverHeaderImage:DisplayObject;		private var _gameOverHeader:Sprite = new Sprite();		private var _backToMenuImage:DisplayObject;		private var _backToMenu:Sprite = new Sprite();		private var _playAgainImage:DisplayObject;		private var _playAgain:Sprite = new Sprite();				private var _addHighscoreImage:DisplayObject;		private var _addHighscore:Sprite = new Sprite();				private var _addHighscoreDialog:AddHighscoreDialog;				private var _score:Number;		private var _scoreText:EasyText;				public function GameOverScreen()		{		}				public function buildObjects():void 		{			_gameOverBg = new GameOverBg();			_gameOver.addChild(_gameOverBg);						_gameOverHeaderImage = new GameOverHeaderImage();			_gameOverHeader.addChild(_gameOverHeaderImage);			_gameOverHeader.x = 40;			_gameOverHeader.y = 150;						_backToMenuImage = new BackToMenuImage();			_backToMenu.addChild(_backToMenuImage);			_backToMenu.addEventListener(MouseEvent.CLICK, backToMenuClick);			_backToMenu.x = 40;			_backToMenu.y = 350;						_playAgainImage = new PlayAgainImage();			_playAgain.addChild(_playAgainImage);			_playAgain.addEventListener(MouseEvent.CLICK, playAgainClick);			_playAgain.x = 40; 			_playAgain.y = 450;						_addHighscoreImage = new AddHighscoreImage();			_addHighscore.addChild(_addHighscoreImage);						_addHighscore.x = 40;			_addHighscore.y = 550;						_scoreText = new EasyText(_score, 20, 0x5E4C28);			_scoreText.x = 270;			_scoreText.y = 260;		}				public function placeObjects():void 		{			addChild(_gameOver);			addChild(_gameOverHeader);			addChild(_backToMenu);			addChild(_playAgain);			addChild(_addHighscore);			addChild(_scoreText);		}				private function playAgainClick(e:MouseEvent):void 		{			(this.parent as MovieClip).gamePlaying = true;			parent.removeChild(this);		}				private function backToMenuClick(e:MouseEvent):void 		{			(this.parent as MovieClip).gamePlaying = false;			(this.parent.parent as MovieClip).removeChild((this.parent.parent as MovieClip).currentGame);		}				public function get addHighscoreDialog():AddHighscoreDialog 		{			return _addHighscoreDialog;		}		public function set addHighscoreDialog(value:AddHighscoreDialog):void 		{			_addHighscoreDialog = value;		}				public function get addHighscore():Sprite 		{			return _addHighscore;		}				public function get score():Number 		{			return _score;		}		public function set score(value:Number):void		{			_score = value;		}	}}