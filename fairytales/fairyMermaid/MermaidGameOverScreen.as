package fairytales.fairyMermaid{	import fairytales.GameOverScreen;	import flash.display.DisplayObject;	import flash.display.Sprite;		public class MermaidGameOverScreen extends GameOverScreen	{		[Embed(source="assets/images/gameOver.png")]		private var GameOverImage:Class;				private var _gameOverImage:DisplayObject;		private var _gameOver:Sprite = new Sprite();		public function MermaidGameOverScreen(s:Number) 		{			score = s;			buildObjects();			placeObjects();			_gameOverImage = new GameOverImage();			_gameOver.addChild(_gameOverImage);			_gameOver.x = 250;			_gameOver.y = 350;			addChild(_gameOver);		}	}}