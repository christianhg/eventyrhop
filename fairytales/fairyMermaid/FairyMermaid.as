package fairytales.fairyMermaid{	import fairytales.Fairytale;	import fairytales.Platform;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;		public class FairyMermaid extends Fairytale	{				public function FairyMermaid(stage:Object)		{			gameStage = stage;				addEventListener(Event.ADDED_TO_STAGE, addedToStageHandler);			}				override public function buildObjects():void		{			addBackground(new MermaidBackground());			addPlatforms();			addHero(new Mermaid());			addVillain(new MermaidVillain());			addPause();		}				override public function addPlatform():void		{			platform = new Shell();		}					override public function addGameOverScreen(value:Number):void		{			gameOverScreen = new MermaidGameOverScreen(value);		}		}}