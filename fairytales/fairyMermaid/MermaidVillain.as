package fairytales.fairyMermaid{	import fairytales.Villain; 		public class MermaidVillain extends Villain	{		//Embed MovieClip		[Embed(source="assets/mermaidVillain.swf", symbol="MermaidVillain_mc")]		public static var MermaidVillainMC:Class;				public function MermaidVillain()		{		}				override public function drawVillain():void		{			view = new MermaidVillainMC();			view.x = -view.width * 0.5;			view.y = -view.height * 0.5;			view.gotoAndStop("front");			this.addChild(view);			height = view.height;		}		}}