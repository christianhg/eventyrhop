package fairytales.fairyClumsy{	import fairytales.Fairytale;	import fairytales.Platform;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;		public class FairyClumsy extends Fairytale	{				public function FairyClumsy(stage:Object)		{			gameStage = stage;				addEventListener(Event.ADDED_TO_STAGE, addedToStageHandler);			}				override public function buildObjects():void		{			addBackground(new ClumsyBackground());			addPlatforms();			addHero(new Clumsy());			addVillain(new ClumsyVillain());			addPause();		}				override public function addPlatform():void		{			platform = new Dirt();		}					override public function addGameOverScreen(value:Number):void		{			gameOverScreen = new ClumsyGameOverScreen(value);		}		}}