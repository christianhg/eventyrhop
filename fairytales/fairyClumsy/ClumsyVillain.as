package fairytales.fairyClumsy{	import fairytales.Villain; 		public class ClumsyVillain extends Villain	{		//Embed MovieClip		[Embed(source="assets/clumsyVillain.swf", symbol="ClumsyVillain_mc")]		public static var ClumsyVillainMC:Class;				public function ClumsyVillain()		{		}				override public function drawVillain():void		{			view = new ClumsyVillainMC();			view.x = -view.width * 0.5;			view.y = -view.height * 0.5;			view.gotoAndStop("front");			this.addChild(view);			height = view.height;		}		}}