package {		import flash.display.DisplayObject;	import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.net.URLRequest;	import flash.display.Loader;		public class Map extends MovieClip 	{			[Embed(source="assets/images/mapBg.png")]		private var MapBgImage:Class;			[Embed(source="assets/images/back.png")]		private var BackImage:Class;				private var _mapBgImage:DisplayObject;		private var _mapBg:Sprite				private var _backImage:DisplayObject;		private var _back:Sprite;				private var mapOverlayLoader:Loader;		private var mapOverlayURLRequest:URLRequest;		public function Map() 		{			buildObjects();			placeObjects();		}				private function buildObjects():void		{			_mapBgImage = new MapBgImage();			_mapBg = new Sprite();			_mapBg.addChild(_mapBgImage);						_backImage = new BackImage();			_back = new Sprite();			_back.addEventListener(MouseEvent.CLICK, backClick);			_back.addChild(_backImage);			_back.x = 128;			_back.y = 620;						mapOverlayLoader = new Loader();			mapOverlayURLRequest = new URLRequest("http://hca.starbeast.dk/app/images/mapOverlay.png");			mapOverlayLoader.load(mapOverlayURLRequest);		}				private function placeObjects():void		{			addChild(_mapBg);			addChild(_back);			addChild(mapOverlayLoader);		}				private function backClick(e:MouseEvent):void 		{			parent.removeChild(this);		}	}}