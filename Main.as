package {	import flash.display.MovieClip;	import flash.display.Sprite;		import MainMenu;	import flash.events.Event;		public class Main extends MovieClip 	{		private var _mainMenu:MainMenu;						public function Main() 		{			buildObjects();			placeObjects();					}				private function buildObjects():void		{			_mainMenu = new MainMenu();			_mainMenu.addEventListener(Event.REMOVED_FROM_STAGE, mainMenuRemovedFromStageHandler);		}				private function placeObjects():void		{			addChild(_mainMenu);		}				private function mainMenuRemovedFromStageHandler(e:Event):void		{			buildObjects();			placeObjects();		}	}}